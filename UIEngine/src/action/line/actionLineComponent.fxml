<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="45.0" prefWidth="897.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="action.line.impl.ActionLineController">
   <children>
      <TextField fx:id="cellidTF" minWidth="40.0" prefHeight="26.0" promptText="Cell ID">
         <HBox.margin>
            <Insets left="5.0" top="5.0" />
         </HBox.margin>
      </TextField>
      <TextField fx:id="originalvalueTF" minWidth="108.0" prefHeight="24.0" promptText="Original Value">
         <HBox.margin>
            <Insets left="15.0" top="5.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="updatevalbtn" minHeight="25.0" minWidth="98.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="98.0" text="Update Value">
         <HBox.margin>
            <Insets left="15.0" top="4.0" />
         </HBox.margin>
      </Button>
      <HBox prefHeight="36.0" prefWidth="238.0">
         <children>
            <Label prefHeight="18.0" prefWidth="205.0" text="Cell's Last Modified Version:">
               <HBox.margin>
                  <Insets top="9.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="lastmodverTF" minWidth="30.0" prefHeight="26.0" prefWidth="110.0" promptText="Version">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </HBox>
      <VBox alignment="CENTER_LEFT" prefHeight="18.0" prefWidth="126.0" spacing="2">
         <HBox.margin>
            <Insets left="15.0" top="5.0" />
         </HBox.margin>
         <children>
            <ComboBox fx:id="versionSelector" promptText="Choose version" styleClass="combo-box">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></ComboBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
