<?xml version="1.0" encoding="UTF-8"?>
<STL-Sheet name="SalesReport">
    <STL-Layout rows="10" columns="5">
        <STL-Size column-width-units="100" rows-height-units="30"/>
    </STL-Layout>
    <STL-Ranges>
        <STL-Range name="Prices">
            <STL-Boundaries from="B2" to="B5"/>
        </STL-Range>
        <STL-Range name="Quantities">
            <STL-Boundaries from="C2" to="C5"/>
        </STL-Range>
    </STL-Ranges>
    <STL-Cells>
        <!-- Headers -->
        <STL-Cell row="1" column="A">
            <STL-Original-Value>Product</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="B">
            <STL-Original-Value>Price</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="C">
            <STL-Original-Value>Quantity</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="1" column="D">
            <STL-Original-Value>Total</STL-Original-Value>
        </STL-Cell>
        
        <!-- Product 1 -->
        <STL-Cell row="2" column="A">
            <STL-Original-Value>Laptop</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="2" column="B">
            <STL-Original-Value>999.99</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="2" column="C">
            <STL-Original-Value>5</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="2" column="D">
            <STL-Original-Value>{TIMES,{REF,B2},{REF,C2}}</STL-Original-Value>
        </STL-Cell>
        
        <!-- Product 2 -->
        <STL-Cell row="3" column="A">
            <STL-Original-Value>Mouse</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="3" column="B">
            <STL-Original-Value>29.99</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="3" column="C">
            <STL-Original-Value>50</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="3" column="D">
            <STL-Original-Value>{TIMES,{REF,B3},{REF,C3}}</STL-Original-Value>
        </STL-Cell>
        
        <!-- Product 3 -->
        <STL-Cell row="4" column="A">
            <STL-Original-Value>Keyboard</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="4" column="B">
            <STL-Original-Value>79.99</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="4" column="C">
            <STL-Original-Value>25</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="4" column="D">
            <STL-Original-Value>{TIMES,{REF,B4},{REF,C4}}</STL-Original-Value>
        </STL-Cell>
        
        <!-- Product 4 -->
        <STL-Cell row="5" column="A">
            <STL-Original-Value>Monitor</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="5" column="B">
            <STL-Original-Value>349.99</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="5" column="C">
            <STL-Original-Value>10</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="5" column="D">
            <STL-Original-Value>{TIMES,{REF,B5},{REF,C5}}</STL-Original-Value>
        </STL-Cell>
        
        <!-- Summary Row -->
        <STL-Cell row="7" column="A">
            <STL-Original-Value>Average Price:</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="7" column="B">
            <STL-Original-Value>{AVERAGE,Prices}</STL-Original-Value>
        </STL-Cell>
        
        <STL-Cell row="8" column="A">
            <STL-Original-Value>Total Units:</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="8" column="B">
            <STL-Original-Value>{SUM,Quantities}</STL-Original-Value>
        </STL-Cell>
        
        <STL-Cell row="10" column="A">
            <STL-Original-Value>Status:</STL-Original-Value>
        </STL-Cell>
        <STL-Cell row="10" column="B">
            <STL-Original-Value>{IF,{BIGGER,{SUM,Quantities},50},High Volume,Low Volume}</STL-Original-Value>
        </STL-Cell>
    </STL-Cells>
</STL-Sheet>
