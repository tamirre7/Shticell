<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_LEFT" spacing="10" stylesheets="@action-commands.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shticell.client.sheethub.components.commands.components.chat.chatcommands.impl.ChatCommandsControllerImpl">
   <padding>
      <Insets top="10" right="10" bottom="10" left="10"/>
   </padding>
   <children>
      <ToggleButton id="auto-updates-button" fx:id="autoUpdatesButton" text="Auto Updates" styleClass="main-action-button" maxWidth="Infinity"/>
      <Button id="Return-button" text="Return" onAction="#returnClicked" styleClass="quit-button" maxWidth="Infinity"/>
   </children>
</VBox>