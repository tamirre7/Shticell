<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #f0f0f0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shticell.client.sheethub.components.loadsheet.impl.LoadSheetControllerImpl">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Label fx:id="greetingLabel" text="Hello, User!" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Label>
   <Button fx:id="loadButton" onAction="#loadButtonClicked" text="Load Sheet" style="-fx-background-color: #4CAF50; -fx-text-fill: white;">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
</VBox>